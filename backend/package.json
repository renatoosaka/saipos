{
  "name": "@reges/sentinela-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node dist/src/main/server.js",
    "prod:seed": "node dist/src/main/seed.js",
    "build": "rimraf dist && yarn tsc",
    "postbuild": "cp ./products.csv ./dist/products.csv",
    "dev:server": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/main/server.ts",
    "dev:seed": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/main/seed.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test --watch -c jest.unit.config.js",
    "test:integration": "yarn test --watch -c jest.integration.config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/amqplib": "^0.5.17",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/mongodb": "^3.6.8",
    "@types/node": "^14.14.28",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.4.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "amqplib": "^0.7.0",
    "bcrypt": "^5.0.1",
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "mongodb": "^3.6.4",
    "mysql2": "^2.2.5",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  }
}
